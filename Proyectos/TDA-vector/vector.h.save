#ifndef VECTOR_H_
#define VECTOR_H_

#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>

#define MAX_SIZE 5


typedef struct {
	int datos[MAX_SIZE];
	int elementosInsertados;
} tVector;


void vectorCrear(tVector *);
bool vectorLleno (const tVector *);
int vectorCantElementos(const tVector *);
bool vectorVacio(const tVector *);
bool vectorInsertarEnOrden(tVector *, int);
void vectorDestruir(tVector *);
bool vectorEliminarElemento(tVector *, int, int *);
void vectorMostrar(const tVector *);


#endif // VECTOR_H_
